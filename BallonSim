local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()local b=a.Load({Title="Balloon Simulator",Style=1,SizeX=400,SizeY=300,Theme="Dark",ColorOverrides={MainFrame=Color3.fromRGB(255,0,0)}})local c=b.New({Title="Main"})local d=b.New({Title="Credits"})c.Toggle({Text="Auto Plow ",Callback=function(e)AutoPlow=e end,Enabled=false})c.Button({Text="Tp Gems",Callback=function()for f,g in pairs(game:GetService("Workspace").Gems:GetDescendants())do if g.Name=="Gem"then g.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame end end end})c.Button({Text="Tp Coins",Callback=function()for f,g in pairs(game:GetService("Workspace").Coins:GetDescendants())do if g.Name=="Coin"then g.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame end end end})c.Button({Text="Tp Coins",Callback=function()for f,g in pairs(game:GetService("Workspace").Coins:GetDescendants())do if g.Name=="Coin"then g.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame end end end})d.Button({Text="                                   MaGiXx#2325",Callback=function()psetclipboard("MaGiXx#2325")end})while wait()do if AutoPlow==true then local h={[1]=1}game:GetService("ReplicatedStorage").FillBalloon:FireServer(unpack(h))end end
